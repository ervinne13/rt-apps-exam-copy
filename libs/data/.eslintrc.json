{
  "extends": ["plugin:@nx/react", "../../.eslintrc.json", "universe/native"],
  "ignorePatterns": ["!**/*", ".expo", "web-build", "cache", "dist"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {
        "import/order": "off"
      }
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
